{
  "name": "Quickly web tools",
  "version": "0.1",
  "description": "Quickly web tools for Dao Xuan Tin",
  "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'sha256-FEid8KRqAfmkqRNM4hJ3fs2WBl20DX8fCust3cv1Rpw='; object-src 'self'",
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
